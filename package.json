    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "set NODE_ENV=development && nest start --watch",
        "dev:macos": "export NODE_ENV=development && nest start --watch",
        "prod:macos": "export NODE_ENV=production && nest build && node ./dist/main",
        "start:debug": "nest start --debug --watch",
        "start:prod": "set NODE_ENV=production && nest build && node ./dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "set NODE_ENV=development && jest",
        "test:watch": "set NODE_ENV=development && jest --watch",
        "test:cov": "set NODE_ENV=development && jest --coverage",
        "test:debug": "set NODE_ENV=development && node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "set NODE_ENV=development && jest --config ./test/jest-e2e.json",
        "postinstall": "tsc --sourceMap false",
        "heroku-postbuild": "npm install --only=dev --no-shrinkwrap && npm run build"
    },
